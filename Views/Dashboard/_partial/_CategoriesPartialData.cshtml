
    <div class="row mt-5">

        <div class="col">

            <div class="card">
                <div class="card-body">
                    <table class="table text-center">
                        <thead>
                            <tt>
                                <td class=text-center>رقم الفئة</td>
                                <td class=text-center>وصف الفئة</td>
                                <td class=text-center>الايقونة</td>
                                <td class=text-center>الرابط</td>
                                <td class="text-center" colspan="2">الإجراء</td>
                
                            </tt>

                        </thead>

                        <tbody>
                            @foreach (var items in Model) {
                                <tr>
                                    <td>
                                    @items.Id 
                                    </td>
                                    <td>
                                    @items.Name  
                                    </td>
                                    <td>
                                    @items.Icon 
                                    </td>
                                    <td>
                                    @items.Url  
                                    </td>
                                    <td>
                                        <a href="#" onclick="showmessage(@items.Id)">
                                            <i class="fa fa-trash text-danger" aria-hidden="true"></i> 
                                        </a>
                                    </td>
                                    <td>
                                        <a asp-action="EditCategories" asp-route-id="@items.Id"><i class="fa fa-pencil text-success" aria-hidden="true"></i></a>
                                    </td>

                                </tr>
                            }
                        </tbody>
                       

                    </div>
                </div>
            </div>
        </div>


    </div>